    "description": "Yii 2 extension wrapper to communicate with RabbitMQ server via AMQP. Based on webtoucher/yii2-amqp",
    "keywords": ["yii", "extension", "amqp", "RabbitMQ"],
    "homepage": "https://github.com/kualkua/yii2-amqp",
    "type": "yii-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Dmitriy Kolokot",
            "email": "dmitriy.kolokot@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "php-amqplib/php-amqplib": "*",
        "webtoucher/yii2-commands": "*"
    },
    "autoload": {
        "psr-4": {
            "webtoucher\\amqp\\": ""
        }
    }
}
